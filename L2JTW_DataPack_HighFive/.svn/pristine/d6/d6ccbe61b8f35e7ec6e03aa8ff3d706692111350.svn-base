<tr><td>SP (%sp%):</td><td><edit var="sp"></td><td><button value="儲存" width=35 action="bypass -h admin_save_npc %npcId% sp $sp" height=15 back="L2UI_CT1.Button_DF_Down" fore="L2UI_CT1.Button_DF"></td></tr>
<tr><td>物攻 (%pAtk%):</td><td><edit var="pAtk"></td><td><button value="儲存" width=35 action="bypass -h admin_save_npc %npcId% pAtk $pAtk" height=15 back="L2UI_CT1.Button_DF_Down" fore="L2UI_CT1.Button_DF"></td></tr>
<tr><td>物防 (%pDef%):</td><td><edit var="pDef"></td><td><button value="儲存" width=35 action="bypass -h admin_save_npc %npcId% pDef $pDef" height=15 back="L2UI_CT1.Button_DF_Down" fore="L2UI_CT1.Button_DF"></td></tr>
